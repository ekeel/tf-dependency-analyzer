    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.16.3",
    "@typescript-eslint/parser": "^5.59.2",
    "@vercel/ncc": "^0.36.1",
    "babel-jest": "^29.6.2",
    "eslint": "^8.39.0",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-jest": "^27.2.1",
